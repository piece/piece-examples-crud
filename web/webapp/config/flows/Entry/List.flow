firstState: ProcessFindList

viewState:
  - name: DisplayList
    view: List
    activity:
      class: Entry_ListAction
      method: doActivityOnDisplayList

actionState:
  - name: ProcessFindList
    activity:
      class: Entry_ListAction
      method: doActivityOnProcessFindList
    transition:
      - event: DisplayListFromProcessFindList
        nextState: DisplayList
